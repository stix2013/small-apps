  "type": "module",
  "devDependencies": {
    "@eslint/compat": "1.2.9",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.27.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.21",
    "@typescript-eslint/eslint-plugin": "8.32.1",
    "acorn": "^8.14.1",
    "browserslist": "^4.24.5",
    "buffer": "^6.0.3",
    "consola": "3.4.2",
    "eslint": "^9.27.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsdoc": "50.6.17",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-vue": "10.1.0",
    "fs-extra": "^11.3.0",
    "mustache": "^4.2.0",
    "picocolors": "1.1.1",
    "resolve": "^1.22.10",
    "rimraf": "^6.0.1",
    "rollup": "^4.41.1",
    "sharp": "^0.34.2",
    "syncpack": "^13.0.4",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "8.32.1",
    "unbuild": "^3.5.0",
    "vue": "^3.5.14"
  },
  "engine-strict": true,
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "keywords": [
    "yellow",
    "yellow mobile",
    "modules",
    "yellow modules",
    "yellow mobile modules",
    "telecomunication",
    "gsm",
    "bundle"
  ],
  "license": "ISC",
  "scripts": {
    "build:stub": "pnpm --filter './packages/**' prepack --stub",
    "cleanup": "npx rimraf -v -g pnpm-lock.yaml dist **/node_modules **/dist apps/web/nuxt-dist **/.nuxt **/.turbo",
    "cleanup:dist": "rimraf -v -g **/dist",
    "cleanup:eslint": "rimraf -v -g **/.eslintcache",
    "cleanup:modules": "rimraf -v -g **/node_modules",
    "cleanup:turbo": "rimraf -v -g **/.turbo",
    "generate:web": "pnpm run web generate",
    "lint": "eslint --ext .vue,.ts,.js,.mjs .",
    "lint:fix": "eslint --ext .vue,.ts,.js,.mjs . --fix",
    "prepack": "FORCE_COLOR=3 pnpm -r prepack",
    "prod:web": "pnpm --filter @yellow-mobile/web run generate:production",
    "staging:web": "pnpm --filter @yellow-mobile/web run generate:staging",
    "web": "pnpm --filter @yellow-mobile/web",
    "typecheck": "tsc --noEmit"
  },
  "packageManager": "pnpm@9.12.3+sha512.cce0f9de9c5a7c95bef944169cc5dfe8741abfb145078c0d508b868056848a87c81e626246cb60967cbd7fd29a6c062ef73ff840d96b3c86c40ac92cf4a813ee"
