{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "prepack"
      ]
    },
    "lint": {
      "outputs": [
        ".eslintcache"
      ]
    },
    "dev:staging": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "prepack"
      ]
    },
    "prepack": {
      "dependsOn": [
        "^prepack"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "build": {
      "dependsOn": [
        "prepack",
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".nuxt/**",
        "!node_modules/.cache"
      ]
    },
    "build:staging": {
      "dependsOn": [
        "prepack",
        "^build:staging"
      ],
      "outputs": [
        "dist/**",
        ".nuxt/**",
        "!node_modules/.cache"
      ]
    },
    "build:staging:standalone": {
      "cache": false,
      "dependsOn": [
        "prepack",
        "^build:staging:standalone"
      ]
    },
    "generate:staging": {
      "dependsOn": [
        "prepack"
      ],
      "outputs": [
        "staging/**",
        ".nutx/**"
      ]
    },
    "generate": {
      "dependsOn": [
        "prepack"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "start:staging": {
      "cache": false
    }
  },
  "globalDependencies": [
    ".env",
    "tsconfig.json"
  ]
}
