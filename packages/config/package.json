  "keywords": [],
  "license": "ISC",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "devDependencies": {
    "@yellow-mobile/eslint-config": "workspace:*",
    "@yellow-mobile/types": "workspace:*",
    "prettier": "3.3.3",
    "unbuild": "^2.0.0",
    "vitest": "^2.1.9"
  },
  "peerDependencies": {
    "typescript": "^5.3.2"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./screens": {
      "import": {
        "types": "./dist/screens.d.mts",
        "default": "./dist/screens.mjs"
      },
      "require": {
        "types": "./dist/screens.d.cts",
        "default": "./dist/screens.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint --cache --ext .js,.ts --ignore-path .eslintignore .",
    "lint:fix": "eslint --cache --ext .js,.ts --ignore-path .eslintignore . --fix",
    "prepack": "pnpm build"
  },
  "sideEffects": false
}
